<div class="portfolio-soundcloud">
  <div class="portfolio-soundcloud-wrapper">
    <div class="portfolio-soundcloud-list">
      <ul>
        {{#each soundCloud}}
          <li>
            <a href="{{this.url}}">{{this.title}}</a>
          </li>
        {{/each}}
      </ul>
    </div>
    <div class="portfolio-playlist">
      <iframe width="100%" height="0" scrolling="no" frameborder="no" src=""></iframe>
    </div>
  </div>
</div>